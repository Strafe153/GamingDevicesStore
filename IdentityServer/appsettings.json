{
  "AppSettings": {
    "AdminPassword": "qwerty"
  },
  "ConnectionStrings": {
    "DatabaseConnection": "Server=.;Database=gaming_store_db;Trusted_Connection=True;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:dd-MM-yyyy HH:mm:ss}, {Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  }
}
